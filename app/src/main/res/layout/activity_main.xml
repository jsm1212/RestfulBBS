<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="570dp"
                android:layout_height="772dp"
                android:background="@drawable/border_line"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent">
                <TextView
                        android:text="ID"
                        android:textSize="25dp"
                        android:gravity="center"
                        android:layout_width="115dp"
                        android:layout_height="75dp" android:id="@+id/textView"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.235" app:layout_constraintVertical_bias="0.357"/>
                <TextView
                        android:text="PWD"
                        android:textSize="25dp"
                        android:gravity="center"
                        android:layout_width="115dp"
                        android:layout_height="75dp" android:id="@+id/textView3"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.235" app:layout_constraintVertical_bias="0.466"/>
                <EditText
                        android:id="@+id/editId"
                        android:textSize="25dp"
                        android:gravity="center"
                        android:layout_width="250dp"
                        android:layout_height="65dp"
                        android:inputType="textPersonName"
                        android:ems="10"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.748" app:layout_constraintVertical_bias="0.364"/>
                <EditText
                        android:id="@+id/editPwd"
                        android:textSize="25dp"
                        android:gravity="center"
                        android:layout_width="250dp"
                        android:layout_height="65dp"
                        android:inputType="textPassword"
                        android:ems="10"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.748" app:layout_constraintVertical_bias="0.472"/>
                <Button
                        android:text="로그인"
                        android:textSize="25dp"
                        android:gravity="center"
                        android:layout_width="349dp"
                        android:layout_height="69dp" android:id="@+id/loginBtn"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.602" app:layout_constraintHorizontal_bias="0.542"/>
                <ImageButton
                        android:id="@+id/kakao_login_button"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/kakao_login_medium_narrow"
                        android:background="@android:color/transparent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.529" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.702"/>
                <Button
                        android:text="회원가입"
                        android:textSize="16dp"
                        android:gravity="center"
                        android:layout_width="195dp"
                        android:layout_height="70dp" android:id="@+id/goRegiBtn"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.218" app:layout_constraintVertical_bias="0.804"/>
                <Button
                        android:text="아이디/비밀번호찾기"
                        android:textSize="16dp"
                        android:gravity="center"
                        android:layout_width="195dp"
                        android:layout_height="70dp" android:id="@+id/goSearchBtn"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.786" app:layout_constraintVertical_bias="0.804"/>
                <TextView
                        android:text="로그인"
                        android:textSize="30dp"
                        android:gravity="center"
                        android:layout_width="207dp"
                        android:layout_height="98dp" android:id="@+id/textView20"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.501" app:layout_constraintVertical_bias="0.161"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>