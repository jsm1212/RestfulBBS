<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegiActivity">

    <TextView
            android:text="회원가입"
            android:textSize="30dp"
            android:gravity="center"
            android:layout_width="200dp"
            android:layout_height="75dp" android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintVertical_bias="0.097"/>
    <TextView
            android:text="아이디"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="130dp"
            android:layout_height="70dp" android:id="@+id/textView10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.148" app:layout_constraintVertical_bias="0.256"/>
    <EditText
            android:id="@+id/regiEditId"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="207dp"
            android:layout_height="71dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.623" app:layout_constraintVertical_bias="0.256"/>
    <TextView
            android:text="패스워드"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="130dp"
            android:layout_height="70dp" android:id="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.148" app:layout_constraintVertical_bias="0.385"/>
    <TextView
            android:text="패스워드확인"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="140dp"
            android:layout_height="70dp" android:id="@+id/textView15"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.148" app:layout_constraintVertical_bias="0.484"/>
    <EditText
            android:id="@+id/regiEditPwd"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="290dp"
            android:layout_height="70dp"
            android:inputType="textPassword"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.79" app:layout_constraintVertical_bias="0.385"/>
    <EditText
            android:id="@+id/regiEditPwd2"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="290dp"
            android:layout_height="70dp"
            android:inputType="textPassword"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.79" app:layout_constraintVertical_bias="0.484"/>
    <EditText
            android:id="@+id/regiEditEmail"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="290dp"
            android:layout_height="70dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.79" app:layout_constraintVertical_bias="0.712"/>
    <EditText
            android:id="@+id/regiEditName"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="290dp"
            android:layout_height="70dp"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.79" app:layout_constraintVertical_bias="0.611"/>
    <TextView
            android:text="이름"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="130dp"
            android:layout_height="70dp" android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.148" app:layout_constraintVertical_bias="0.608"/>
    <TextView
            android:text="이메일"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_width="130dp"
            android:layout_height="70dp" android:id="@+id/textView13"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.148" app:layout_constraintVertical_bias="0.714"/>
    <Button
            android:id="@+id/regiBtn"
            android:textSize="25dp"
            android:gravity="center"
            android:text="가입"
            android:layout_width="190dp"
            android:layout_height="60dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.823" app:layout_constraintHorizontal_bias="0.487"/>
    <Button
            android:text="중복확인"
            android:textSize="15dp"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/duplBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.918" app:layout_constraintVertical_bias="0.259"/>
    <TextView
            android:id="@+id/duplText"
            android:gravity="center"
            android:layout_width="174dp"
            android:layout_height="29dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.612" app:layout_constraintVertical_bias="0.321"/>
</androidx.constraintlayout.widget.ConstraintLayout>